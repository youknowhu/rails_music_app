<form action="<%= band_url(@band) %>" method= "POST">
    <input
    type="hidden"
    name="_method"
    value="PATCH">

    <input
    type="hidden"
    name="authenticity_token"
    value='<%= form_authenticity_token %>'>

  <label for="band"> Band Name
    <input type="text" name="band[name]" value="<%=@band.name%>" >
  </label>

  <button>Update Band</button>

</form>
